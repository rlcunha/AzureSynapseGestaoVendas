				"source": [
					"spark = SparkSession.builder.getOrCreate()\r\n",
					"w = Window().orderBy(f.lit('A'))"
				],
				"execution_count": 23
			},
			{
				"cell_type": "markdown",
				"metadata": {
					"nteract": {
						"transient": {
